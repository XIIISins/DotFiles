sc() {
  $HOME/.scripts/setcolor "$@"
}

#BG_COLOR=#1e4156
#INACTIVE_BG_COLOR=#142a38
#TEXT_COLOR=#f3f4f5
#INACTIVE_TEXT_COLOR=#676E7D
#URGENT_BG_COLOR=#E53935
#BAR_BG_COLOR=#18242C

BG_COLOR=$(sc BG_COLOR)
INACTIVE_BG_COLOR=$(sc INACTIVE_BG_COLOR)
TEXT_COLOR=$(sc TEXT_COLOR)
INACTIVE_TEXT_COLOR=$(sc INACTIVE_TEXT_COLOR)
URGENT_BG_COLOR=$(sc URGENT_BG_COLOR)
BAR_BG_COLOR=$(sc BAR_BG_COLOR)
